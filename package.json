{
  "name": "blog.kochie.io",
  "version": "1.0.0",
  "description": "My blog, written in nextjs typescript.",
  "repository": {
    "type": "git",
    "url": "https://github.com/kochie/blog.kochie.io.git"
  },
  "scripts": {
    "test": "jest",
    "tcm": "tcm -p src/**/*.scss",
    "dev": "now dev",
    "dev:next": "next",
    "lint": "npm run prettier && npm run eslint",
    "prettier": "prettier --write \"**/*.{tsx,ts,less,js,jsx,json}\"",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "keywords": [
    "blog",
    "nextjs",
    "static"
  ],
  "author": "Robert Koch <robert@kochie.io>",
  "license": "MIT",
  "engines": {
    "node": ">8.10.x"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-pro": "^5.13.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-regular-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/pro-light-svg-icons": "^5.13.0",
    "@fortawesome/pro-regular-svg-icons": "^5.13.0",
    "@fortawesome/pro-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@mdx-js/loader": "^1.5.8",
    "@mdx-js/mdx": "^1.5.8",
    "@next/mdx": "^9.3.2",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.5",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "@zeit/next-mdx": "^1.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "husky": "^4.2.3",
    "intl-list-format": "^1.0.3",
    "jest": "^25.2.3",
    "next": "^9.3.2",
    "next-compose-plugins": "^2.2.0",
    "next-offline": "^5.0.0",
    "next-seo": "^4.4.0",
    "node-sass": "^4.13.1",
    "prettier": "^2.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-ga": "^2.7.0",
    "react-test-renderer": "^16.13.1",
    "ts-jest": "^25.2.1",
    "typed-css-modules": "^0.6.3",
    "typescript": "^3.8.3",
    "typescript-estree": "^18.1.0"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run tcm && npm run lint",
      "pre-push": ""
    }
  },
  "jest": {
    "preset": "ts-jest"
  },
  "dependencies": {}
}
